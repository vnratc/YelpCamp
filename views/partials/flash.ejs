<% if (success.length || error.length) { %>
    <div class="alert <%= success.length ? "alert-success" : "alert-warning"%> alert-dismissible fade show" role="alert">
        <%= success %>
        <%= error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
 <% } %>
